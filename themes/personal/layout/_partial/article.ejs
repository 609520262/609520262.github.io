    $(document).ready(function() {
        $(".wy-menu-vertical li a").each(function() {
            $(this).removeClass('active');

            if ($($(this))[0].href == String(window.location)) {

                $(this).addClass('active');
                $(this).parents('details').attr('open', true);



            }
        });
		if (window.location.pathname.includes('/docs/en/')){
		
		$(".button-wrap span:last-child").text("English (US)");
		$(".wy-menu-vertical summary").text('Entropy theory');
		
		var second_title = ['Quick start entropy','Entropy theory','About us'];
		$(".wy-menu-vertical .caption-text").each(function(index, domEle) {

        $(domEle).text(second_title[index]);
    });
		
		var englist_item = $(".wy-menu-vertical .reference.internal");
		englist_item.each(function(){
			var temp = $(this).text().match(/\(([^)]*)\)/);
			$(this).text(temp[1]);
			});
		
		
		
		}
		else{
		$(".button-wrap span:last-child").text("中文(简体)");

		}
		$("#languages-switcher-button").click(function(){
		if ($(".button-wrap span:last-child").text() ==="中文(简体)"){
		
		$(location).attr('href','/docs/en/');
		
		
		
		}
		else{
		
		$(location).attr('href','/docs/');
		
		}
		});

        var articleBody = $('div.document');
        var documenTocList = $('.document-toc-list');
        articleBody.find('h2').each(function() {

            var item = $('<li class="document-toc-item "><a class="document-toc-link" href=' + $(this).children('a').attr('href') + '>' + $(this).text().slice(0, -1) + '</a></li>')
            documenTocList.append(item);
            var text = '[href=' + $(this).children('a').attr('href') + ']';
            $('.toc').find(text).removeClass('active');

        });
	

        function getPos(obj) {

            var t = 0;
            while (obj) {
                t = t + obj.offsetTop;

                obj = obj.offsetParent;
            }
            return {
                top: t
            };

        }

        $(window).scroll(function() {

            articleBody.find('h2').each(function() {
                console.log(getPos($(this)[0]));
                console.log("scrollTop:" + $(window).scrollTop());
                if ($(window).scrollTop() >= getPos($(this)[0]).top) {

                    var text = '[href=' + $(this).children('a').attr('href') + ']';
                    console.log(text);

                    $('.toc').find('a').removeClass('active');
                    $('.toc').find(text).addClass('active');
                }

            })






        });
		

    })
</script>